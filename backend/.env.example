# OpenAI API Configuration (Legacy - Switching to Gemini)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-5-nano

# Gemini AI Configuration (Google AI Studio)
GOOGLE_API_KEY=your_google_api_key_here
GEMINI_MODEL=gemini-1.5-flash

# Supabase Production Configuration (Get these from supabase.com)
# Free Tier: 500MB DB, 1GB Storage
SUPABASE_URL=your_supabase_project_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Security & CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# Demo / Mock Settings
# Set to 'true' to use simulated AI responses (Zero-Cost mode)
IS_DEMO_MODE=false
